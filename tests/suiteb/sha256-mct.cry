import SuiteB

sha256_mct : [256] -> [100][256]
sha256_mct seed0 = take`{100} xs
  where
    xs = drop`{1} (iterate sha256_mct_round seed0)

sha256_mct_round : [256] -> [256]
sha256_mct_round seed = ys!0
  where
    xs   = [seed,seed,seed] # ys
  
    ys : [1000][256]
    ys@i = sha256 (xs@i # xs@(i+1) # xs@(i+2))

property sha256_mct_correct =
  sha256_mct_test_output ==
