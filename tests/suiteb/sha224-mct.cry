import SuiteB

sha224_mct : [224] -> [100][224]
sha224_mct seed0 = take`{100} xs
  where
    xs = drop`{1} (iterate sha224_mct_round seed0)

sha224_mct_round : [224] -> [224]
sha224_mct_round seed = ys!0
  where
    xs   = [seed,seed,seed] # ys
  
    ys : [1000][224]
    ys@i = sha224 (xs@i # xs@(i+1) # xs@(i+2))


property sha224_mct_correct =
  sha224_mct_test_output ==
  sha224_mct 0x3d4cd83b6b83355ca34fb473de56b5721d27d984ab6f67e6a36feff8

