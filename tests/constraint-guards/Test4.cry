test : {n, a} [n]a -> Bit
test x = propguards
  | (n - 5 >= 0, n >= 5) => True
  | 0 == 0 => False

property test_True = 
  (test [1..5] == True) &&
  (test [1..6] == True) &&
  (test [1..7] == True) &&
  (test [1..8] == True)

property test_False = 
  (test [1..1] == False) &&
  (test [1..2] == False) &&
  (test [1..3] == False) &&
  (test [1..4] == False)
